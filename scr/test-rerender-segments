#! /usr/bin/env bash

if [ -z "$GITHUB_WORKSPACE" ]; then
    source lib-path-manipulation
else
    source /tmp/baca/scr/lib-path-manipulation
fi

wrapper="$GITHUB_WORKSPACE"
segments=$(realpath "$1")
directories=$(ls -d "$segments"[0-9]*)

echo Wrapper is "$wrapper"
echo Segments is "$segments"
echo Directories is "$directories"
