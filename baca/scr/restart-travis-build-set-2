    curl -s -X POST \
    -H "Content-Type: application/json" \
    -H "Accept: application/json" \
    -H "Travis-API-Version: 3" \
    -H "Authorization: token nXLfNt4QfLfWrbteWEGsiw" \
    -d "$body" \
    https://api.travis-ci.org/repo/trevorbaca%2F$repo/requests
done
