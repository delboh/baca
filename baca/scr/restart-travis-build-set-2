body='{
"request": {
  "branch":"master"
}}'

# TRAVIS LIMITS TO 10 BUILDS AT A TIME
for repo in \
    ikribu imaginaire ins-wasser khamr krummzeit \
    lidercfeny manifolds manos mraz myrkr
